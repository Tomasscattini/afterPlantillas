<table class="table">
  <thead>
    <tr>
      <th scope="col">Titulo</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
    {{#if productos}}
    {{#each productos}}
    <tr>
      <th scope="row">1</th>
      <td>{{title}}</td>
      <td>{{price}}</td>
      <td>{{thumbnail}}</td>
    </tr>
    {{/each}}
    {{/if}}
  </tbody>
</table>